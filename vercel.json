{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static-build"
    },
    {
      "src": "produtos_iniciais.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/produtos_iniciais.json",
      "dest": "/produtos_iniciais.json"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}